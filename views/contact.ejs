<body class="d-flex h-1 text-center text-bg-dark"> 
<div class="container">
      

    <form id="contact-form" class="needs-validation">
  <div class="row m-1">  
  <div class="col m-3">
    <label for="contact-last" class="form-label">First Name</label>
    <input type="text" class="form-control" id="contact-first"required>
    <div class="invalid-feedback">
    Please enter your first name.
  </div>
  </div>
  <div class="col m-3">
    <label for="contact-last" class="form-label">Last Name</label>
    <input type="text" class="form-control" id="contact-last"required>
    <div class="invalid-feedback">
    Please enter your last name.
  </div>
</div>
</div>
<div class="row m-1">
  <div class="mb-3">
    <label for="contact-eamil-addr" class="form-label">Email address</label>
    <input  type="email" class="form-control" id="contact-eamil-addr" aria-describedby=" contact-email-help" placeholder="name@example.com"required>
      <div id="emailFeedback" class="invalid-feedback">
        Format should be example@example.com.
      </div>
    <div id="emailHelp" class="form-text"></div>
  </div>
</div>

<div class="row m-1">
  <div class="m-1">
    <label for="contact-question" class="form-label"> Question or Comment</label>
    <div class="input-group has-validation">
  <textarea class="form-control" id="contact-question" rows="5" maxlength="1000" aria-describedby="contact-help" required></textarea>
  <div id="contacct-question-help" class="invalid-feedback">
    Please enter a question or comment.
</div>
</div>
</div>
</div>

<div class="col m-3">
</div> 
 <button id="send-contact" name="sndContact" class="btn btn-primary" type="submit">Submit Request</button>
